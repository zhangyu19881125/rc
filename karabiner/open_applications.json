{
  "title": "open applications",
  "rules": [
    {
      "description": "open chrome",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
                "shell_command": "open -a \"Google Chrome\""
            }
          ]
        }
      ]
    },
