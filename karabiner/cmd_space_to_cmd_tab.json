{
  "title": "Ctrl/Option/Command + Tab Change",
  "rules": [
    {
      "description": "Command-space to command-Tab Change",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "command"
              ]
            }
          ]
        }
      ]
    }
  ]
}
